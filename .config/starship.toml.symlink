format = """
(\
$git_branch\
$git_status\
$rust\
$golang\
$terraform\
\n\
)\
[▲](bold brightwhite)\
[$directory](white)\
$character"""

[character]
format = ''

[aws]
symbol = "  "

[buf]
symbol = " "

[c]
symbol = " "

[conda]
symbol = " "

[dart]
symbol = " "

[directory]
style = "fg:#ffffff"
format = "[ $path ]($style)"
truncation_length = 3
truncation_symbol = "…/"
read_only = " "

[docker_context]
symbol = " "

[elixir]
symbol = " "

[elm]
symbol = " "

[fossil_branch]
symbol = " "

[git_branch]
format = """
[](fg:black bg:blue)\
[ $symbol$branch(:$remote_branch) ]($style)\
[](fg:blue bg:none) """
style = "bg:blue fg:black"
symbol = " "

[golang]
symbol = " "

[guix_shell]
symbol = " "

[haskell]
symbol = " "

[haxe]
symbol = "⌘ "

[hg_branch]
